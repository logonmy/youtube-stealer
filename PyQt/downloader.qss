QTabBar::tab{
	width: 100;
	height: 25;
	background:rgba(117,117,122,70%);
	border-top-width: 3px;
	border-right-width: 0px;
	border-left-width: 0px;
	border-bottom-width: 0px;
	border-color: white;
	padding:5px,10px,5px,10px;
	font-size: 15px;
}
QTabWidget::tab:selected{
	width: 100;
	height: 25;
	background:rgba(197,197,201,70%);
	border-top-width: 3px;
	border-right-width: 0px;
	border-left-width: 0px;
	border-bottom-width: 0px;
	border-color: white;
	padding:5px,10px,5px,10px;
	font-size: 15px;
}



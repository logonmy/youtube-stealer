/*****************setting widget********************/

QLabel[name='lab']{
	background: rgba(0,0,0,1%);
	border:0px;
	color:white;
	font-size: 18px;
	text-align: center;

}

QCheckBox[name='check']{
	border:0px;
	background: rgba(0,0,0,1%);
	font-size: 14px;
	color:white;
}
QCheckBox[name='check']::indicator:unchecked{
	background-image: image url(components/icon.png);
}
QCheckBox[name='check']::indicator:checked{
	background-image: image url(components/icon.png);
}

QLineEdit#line_3{
	height: 50px;
	width:100px;
	border-bottom-width: 3px;
	background: rgba(150,150,150,1%);
